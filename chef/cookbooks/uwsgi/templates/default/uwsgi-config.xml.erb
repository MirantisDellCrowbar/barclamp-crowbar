<uwsgi>
    <% @options.sort_by{|sym| sym.to_s}.each do |key, value| %><% if value == true %>
        <<%= key %>/>
    <% else %>
        <<%= key %>><%= value %></<%= key %>>
    <% end %><% end %>
    <% @instances.each_with_index do |instance, index| %><% instance.sort_by{|sym| sym.to_s}.each do |key, value| %><% if value == true %>
        <<%= key %> id="<%= index %>"/>
    <% else %>
        <<%= key %> id="<%= index %>"><%= value %></<%= key %>>
    <% end %><% end %><% end %>
</uwsgi>
